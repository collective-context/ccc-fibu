&ADD_MATCH  (+-) = "Bezeichnung                    ", 5, KURZ_BEZ
  &DATA_H   (+-) = 5, FI_LIEFER

&REF_KEY    (+)  = 
  &DATA_H   (+-) = 5, KURZ_BEZ

