>>Dummy Felder fr Match-Code-Suche
&REF_KEY    (+)    >>Kontonummer Sachkontenstamm
&REF_MATCH  (+-) = "Konto-Nr.  ", 3, KONTONR
&ADD_MATCH  (+-) = "Bezeichnung                    ", 3, KONTO_BEZ
  &DATA_H   (+-) = 3, KONTONR

&REF_KEY    (+)
  &DATA_H   (+-) = 3, KONTO_BEZ

&REF_KEY    (+)    >>Kontonummer Kundenstamm
&REF_MATCH  (+-) = "Konto-Nr.  ", 4, FI_KUNDE
&ADD_MATCH  (+-) = "Bezeichnung                    ", 4, KURZ_BEZ
  &DATA_H   (+-) = 4, FI_KUNDE

&REF_KEY    (+)  = 
  &DATA_H   (+-) = 4, KURZ_BEZ

&REF_KEY    (+)    >>Kontonummer Lieferantenstamm
