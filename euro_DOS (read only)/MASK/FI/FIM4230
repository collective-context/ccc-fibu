  &DATA     (+-) = 1, _KTO_ART     >>Kontoart
  &DEFAULT  (+-) = "s"
  &CHOICE   (+-) = "ÄArt des Personenkontos", "Kundenkonto",k,\
                   "Lieferantenkonto",l,  "Sachkonto",s

>>Von Kontomunner S
  &HELPTEXT (+)  = FIHLP10, 413, 00
  &DATA     (+-) = 1, _V_KTONR
>>&DEFAULT  (+-) = "0000"

>>Bis Kontonummer S
  &HELPTEXT (+)  = FIHLP10, 413, 1
  &DATA     (+-) = 1, _B_KTONR
>>&DEFAULT  (+-) = "9999"

>>Kopien
  &HELPTEXT (+)  = SYHLP10, 402, 04
  &DATA     (+-) = 1, _KOPIEN
  &DEFAULT  (+-) = "1"

>>von Seite
  &HELPTEXT (+)  = SYHLP10, 400, 1
  &DATA     (+-) = 1, _VON_SEITE
  &DEFAULT  (+-) = "0001"

>>bis Seite
  &HELPTEXT (+)  = SYHLP10, 400, 1
  &DATA     (+-) = 1, _BIS_SEITE
  &DEFAULT  (+-) = "9999"

