  &DATA     (+-) = 1, _V_SATZNR
  &DEFAULT  (+-) = "000001"

>>Bis Satznummer
  &HELPTEXT (+)  = FIHLP10, 421, 00
  &DATA     (+-) = 1, _B_SATZNR
  &DEFAULT  (+-) = "999999"

>>                Von Konto-Nr: S@@@@@@@@  Bis Konto-Nr: S@@@@@@@@
>>Von Kontomunner S
>>  &HELPTEXT (+)  = FIHLP10, 421, 00
>>  &DATA     (+-) = 1, _V_KTONR

>>Bis Kontonummer S
>>  &HELPTEXT (+)  = FIHLP10, 421, 1
>>  &DATA     (+-) = 1, _B_KTONR

>>Kopien
  &HELPTEXT (+)  = SYHLP10, 402, 04
  &DATA     (+-) = 1, _KOPIEN
  &DEFAULT  (+-) = "1"


>>von Seite
  &HELPTEXT (+)  = STHLP10, 413, 9
  &DATA     (+-) = 1, _VON_SEITE
  &DEFAULT  (+-) = "0001"

>>bis Seite
  &HELPTEXT (+)  = STHLP10, 413, 10
  &DATA     (+-) = 1, _BIS_SEITE
  &DEFAULT  (+-) = "9999"


