  &HELPTEXT  (+)  = FIHLP10, 021, 02
  &DATA      (+-) = 0, KLASSE_5
  &DEFAULT   (+-) = "5 Materialaufwand und Aufwendungen für bezogene Leistungen"

>>Kontoklassen 0-9
  &HELPTEXT  (+)  = FIHLP10, 021, 02
  &DATA      (+-) = 0, KLASSE_6
  &DEFAULT   (+-) = "6 Personalaufwand"

