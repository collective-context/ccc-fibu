>>Mask:   STM020
>>Prog.:  ST1100.C
>>Update: 6.08.91

&TITEL    = "ST  STAMM Anlegen Kontoplan"
&FRAME    = 0,0,0,0, "Anlegen/Ändern Kontoplan", 2
>>&SEQUENZ= 00,01,02,04,06,09,11,14,17,19,20,22,23,25,26,27

&MASKHELP = FIHLP10, kons-an
&INDEX    = "BASIS"

&DATEI      (0)  = MASK, D.SYP_50/A

>>Mandanten-Nummer (S)
&REC_KEY     (0)
&REC_MATCH   (+-) = "Mandanten-Nr.", 0, MAND_50
&ADD_MATCH   (+-) = "Klasse - Zeile 0              ", 0, KLASSE_0
  &HELPTEXT  (+-) = SYHLP10, 041, 00
  &DATA      (+-) = 0, MAND_50

>>Kontoklassen 0-9
  &HELPTEXT  (+)  = FIHLP10, 021, 02
  &DATA      (+-) = 0, KLASSE_0
  &DEFAULT   (+-) = "0 Anlagevermögen und Aufw. für Betriebsingangsetzung/-erweiterung"

>>Kontoklassen 0-9
  &HELPTEXT  (+)  = FIHLP10, 021, 02
  &DATA      (+-) = 0, KLASSE_1
  &DEFAULT   (+-) = "1 Vorräte"

>>Kontoklassen 0-9
  &HELPTEXT  (+)  = FIHLP10, 021, 02
  &DATA      (+-) = 0, KLASSE_2
  &DEFAULT   (+-) = "2 Sonstiges Umlaufvermögen und Rechnungsabgrenzungsposten"

>>Kontoklassen 0-9
  &HELPTEXT  (+)  = FIHLP10, 021, 02
  &DATA      (+-) = 0, KLASSE_3
  &DEFAULT   (+-) = "3 Verbindlichkeiten, Rückstellungen u. Rechnungsabgrenzungsposten"

>>Kontoklassen 0-9
  &HELPTEXT  (+)  = FIHLP10, 021, 02
  &DATA      (+-) = 0, KLASSE_4
  &DEFAULT   (+-) = "4 Betriebliche Erträge"

