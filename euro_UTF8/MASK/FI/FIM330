&PARA_READ  (3)  = MASK, D.SYP_50  >>Konsten des aktuellen Jahres

>>--- Positionsteil
>>Konto-Nummer
&REC_KEY    (0)
&REC_MATCH  (+-) = "Konto-Nummer", 0, KONTONR
&ADD_MATCH  (+-) = "Konten-Bezeichnung            ", 0, KONTO_BEZ
  &HELPTEXT (+-) = FIHLP10, 130, 0
  &DATA     (+-) = 0, KONTONR

>>Konten-Bezeichnung
&REC_KEY    (+)
&REC_MATCH  (+-) = "Konten-Bezeichnung            ", 0, KONTO_BEZ
&ADD_MATCH  (+-) = "Konto-Nummer", 0, KONTONR
  &HELPTEXT (+-) = FIHLP10, 130, 1
  &DATA     (+-) = 0, KONTO_BEZ

>>Saldovortrag
  &DATA     (+)  = 0, VOR_SALDO

>>Saldovortrag
  &HELPTEXT (+) = FIHLP10, 112, 1
  &DATA     (+-) = 1, _VOR_SALDO

>>Sammel-Kennz.      (0/1)
>>&HELPTEXT (+)  = FIHLP10, 220, 4
>>&DATA     (+-) = 1, _EIN_AUS
>>&DEFAULT  (+-) = "e"
>>&CHOICE   (+-) = "Saldovortrag",  "ein",e,   "aus",a

>>зддддддд  1. Zeile: ддддддд©
  &DATA_H   (+)  = 2, MAND_01     >>Mandanten-Nummer
  &DATA_H   (+)  = 2, UST_1       >>Steuersatz 1 - 8
