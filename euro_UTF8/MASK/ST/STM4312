  &DATA     (+-) = 1, _W_BRANCHE
  &MODUS    (+-) = PREPARE
  &SELECT   (+-) = "Gruppe",\
                 "Statistik",               1, "Eigene Kraft",            2,\
                 "-''- mit Brief",          4, "Privat",                  8,\
                 "frei 1",                 16, "frei 2",                 32


>>Persönlichkeiten (Werbung erhalten)
  &HELPTEXT (+)  = AKVHLP10, 110, 10
  &DATA     (+-) = 1, _W_WERB_EI
  &SELECT   (+-) = "Persönlich / wir haben bekommen",\
                 "Weihnachts-Billet",       1, "Weihnachts-Geschenk",     2,\
                 "Auf Weihn.Wü. geantw.",   4, "W-Wünsche o.eig.Schr.",   8,\
                 "Gratulationsschreiben",  16,

